<h1>Welcome, <%= current_admin.full_name %>!</h1>
<% if current_admin.is_admin? %>
  <p>You're logged in as an admin.</p>
<% elsif current_admin.is_officer? %>
  <p>You're logged in as an officer.</p>
<% else %>
  <p>You're logged in as a regular user.</p>
<% end %>

<%= link_to "Sign Out", destroy_admin_session_path %>
<p style="color: red"><%= alert %></p>
<p style="color: green"><%= notice %></p>

<h1>Checks#index</h1>
<p>Find me in app/views/checks/index.html.erb</p>

<div id="checks">
  <% @checks.each do |check| %>
    <%= render check %>
    <p>
      <%= link_to "Show this form", check %>
    </p>
  <% end %>
</div>

<%= link_to "New form", new_check_path %>
<%= link_to "View sub-accounts", sub_accounts_path %>
<%= link_to "Admin Page", admins_admin_index_path %>
